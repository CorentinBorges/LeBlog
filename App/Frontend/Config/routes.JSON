{
	"routes":
	[
		{
			"url":"/blog/",
			"module":"Home",
			"action":"index"
		},
		{
			"url":"/blog/CV",
			"module":"Home",
			"action":"CV"
		},
		{
			"url":"/blog/posts",
			"module":"Posts",
			"action":"index"
		},
		{
			"url":"/blog/post([0-9])+",
